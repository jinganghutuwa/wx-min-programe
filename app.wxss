/**app.wxss**/
page{
  font-size: 28rpx;
  color: #333;
  background-color: #EFEFF1;
}
page,view,text{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
input,button {
  outline: none;
  border: none;
}
.flex,[class*='flex-'],[class^='flex-']{
  display: flex;
  display: -webkit-flex;
  display:-ms-flexbox;
}
.flex-wrap{
  flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
}
.flex-btwn-aic{
  align-items: center;
  -webkit-align-items: center;
  -ms-align-items: center;
  -webkit-justify-content: space-between;
  -ms-justify-content: space-between;
  justify-content: space-between;
}
.flex-btwn{
  -webkit-justify-content: space-between;
  -ms-justify-content: space-between;
  justify-content: space-between;
}
.flex-spar-aic{
  align-items: center;
  -webkit-align-items: center;
  -ms-align-items: center;
  -webkit-justify-content: space-around;
  -ms-justify-content: space-around;
  justify-content:space-around;
}

.flex-jcc-aic{
  align-items: center;
  -webkit-align-items: center;
  -ms-align-items: center;
  -webkit-justify-content: center;
  -ms-justify-content: center;
  justify-content: center;
}
.flex-jcc{
  -webkit-justify-content: center;
  -ms-justify-content: center;
  justify-content: center;
}
.flex-jce{
  -webkit-justify-content: flex-end;
  -ms-justify-content: flex-end;
  justify-content: flex-end;
}
.flex-aic{
  align-items: center;
  -webkit-align-items: center;
  -ms-align-items: center;
}
.flex-1{
  flex: 1;
  -webkit-flex:1;
  -ms-flex:1;
  -moz-flex:1;
}
.overHidden{
  overflow: hidden
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.color-red{
  color: #FF5A5A;
}
.color-999{
  color: #999999;
}
.footer-btn{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 96rpx;
  background-color: #0271C3;
  text-align: center;
  line-height: 96rpx;
  color: #fff;
  font-size: 30rpx;
}
.submit-btn{
  margin: 160rpx 30rpx 0;
  height: 96rpx;
  background-color:#0271C3;
  border-radius: 48rpx;
  color: #fff;
   font-size: 30rpx;
  text-align: center;
  line-height: 96rpx;
}
.loadding{
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 30rpx;
}
.no-data{
  text-align: center;
  font-size: 32rpx;
  margin-top: 400rpx;
}
